# Airtable Configuration
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here
AIRTABLE_TABLE_NAME=Style Profiles
# Get your API key from https://airtable.com/account
# Get your Base ID from your Airtable base URL or API documentation

# ActiveCampaign Configuration (Optional - if you want to still collect emails)
AC_API_KEY=your_activecampaign_api_key_here
AC_BASE_URL=https://your-account.api-us1.com
# Get your base URL from ActiveCampaign Settings > Developer

# Custom Field IDs from ActiveCampaign
# These are the field IDs you create in ActiveCampaign for quiz results
AC_FIELD_PRIMARY_ARCHETYPE=1
AC_FIELD_SECONDARY_ARCHETYPE=2
AC_FIELD_QUIZ_RESULT=3
AC_FIELD_QUIZ_DATE=4

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000